function TIC()

				cls(12)

				for i = 0, 15 do
				    if i == 12 then -- 12 is white
								    rect(0, i * 8, 160, 8, 0)
								end
								
        print(i, 64, i * 8, i)
        rect(0, i * 8, 60, 8, i)
        rect(80, i * 8, 160, 8, i)
    end

end
